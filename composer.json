{
    "name": "longtimejones/toucanine",
    "type": "project",
    "keywords": ["framework", "hmvc", "router"],
    "description": "A very simple, yet flexible, PHP5 HMVC-alike micro framework.",
    "homepage": "https://longtimejones.github.io/toucanine/",
    "license": "MIT",
    "authors": [
        {
            "name": "Tim Jong Olesen",
            "email": "tim@olesen.be"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ezyang/htmlpurifier": "~4.9",
        "illuminate/database": "~4.2",
        "nette/http": "~2.4"
    },
    "conflict": {
        "nette/utils": ">=3.0",
        "symfony/polyfill-mbstring": ">=1.2",
        "symfony/translation": ">=4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "ToucaNine\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
